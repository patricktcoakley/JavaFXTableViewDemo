<AnchorPane prefHeight="449.0" prefWidth="465.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="EditActorViewController">
   <children>
      <BorderPane layoutY="49.0" prefHeight="401.0" prefWidth="758.0">
         <center>
            <GridPane prefHeight="314.0" prefWidth="758.0" BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="First Name" />
                  <Label text="Last Name" GridPane.rowIndex="1" />
                  <Label text="Film Title" GridPane.rowIndex="2" />
                  <Label text="Release Year" GridPane.rowIndex="3" />
                  <TextField fx:id="firstNameTextField" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="25.0" prefWidth="350.0" GridPane.columnIndex="1" />
                  <TextField fx:id="lastNameTextField" maxHeight="-Infinity" maxWidth="-Infinity" prefWidth="350.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="titleTextField" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="25.0" prefWidth="350.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <TextField fx:id="releaseYearTextField" maxHeight="-Infinity" maxWidth="-Infinity" prefWidth="350.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
               </children>
            </GridPane>
         </center>
         <bottom>
            <ButtonBar maxWidth="-Infinity" prefHeight="65.0" prefWidth="330.0" BorderPane.alignment="CENTER">
              <buttons>
                <Button fx:id="saveButton" mnemonicParsing="false" text="Save" onAction="#saveButtonHandler"/>
                  <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" onAction="#cancelButtonHandler" />
              </buttons>
            </ButtonBar>
         </bottom>
      </BorderPane>
